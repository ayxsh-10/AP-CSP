#title
print("Temperature Warning System")
#get input & str -> float
try:
    temp = float(input("Enter temperature: "))
#classifier
    if temp > 100 or temp < 0:
        print("EXTREME TEMPERATURE.")
    elif temp <= 32:
        print("Pretty Cold")
    elif temp <= 50:
        print("Chilly")
    elif temp <= 70:
        print("Cool")
    elif temp <= 85:
        print("Hot")
    else:
        print("Extremely Hot")
#syntax statement
except ValueError:
    print("This value is not accepted, please input a number.")