#title
print("Grade Checker")
#get users test score
gradeInput = input("Enter your test score: ")
#str -> int
if gradeInput.isdigit():
    grade = int(gradeInput)
#safety statement
else:
    print("Invalid Input")
    grade = -1
#classifier
if grade >= 90 and grade <= 100:
    print("A")
elif grade < 90 and grade >= 80:
    print("B")
elif grade < 80 and grade >= 70:
    print("C")
elif grade < 70 and grade >= 60:
    print("D")
elif grade < 60 and grade >= 0:
    print("F")
else:
    print("You have inputted your answer incorrectly.")